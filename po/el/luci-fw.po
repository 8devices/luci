msgid "External Zone"
msgstr "Εξωτερική Ζώνη"

#. External port
#: applications/luci-fw/luasrc/i18n/luci-fw.en.lua:30
msgid "External port"
msgstr "Εξωτερική θύρα"

#. Source MAC
#: applications/luci-fw/luasrc/i18n/luci-fw.en.lua:33
msgid "Source MAC"
msgstr "MAC πηγής"

